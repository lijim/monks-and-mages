{
  "compilerOptions": {
    "module": "es2022",
    "lib": ["es2021", "dom", "ES2021.String"],
    "esModuleInterop": true,
    "target": "es2021",
    "jsx": "react",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
  },
  "include": ["src/**/*"]
}