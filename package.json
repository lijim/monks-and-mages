    "description": "Monks and Mages",
    "main": "dist/server.bundle.js",
    "private": true,
    "scripts": {
        "build": "npx webpack",
        "scrapeImages": "npx esrun src/scripts/scrapeImages.ts",
        "compressImages": "npx esrun src/scripts/compressImages.ts",
        "ci": "yarn lint && jest --maxWorkers=2",
